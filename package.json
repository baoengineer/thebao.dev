{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf build public/build",
    "dev:css": "postcss app/styles/index.css --dir public/styles -w",
    "build:css": "postcss app/styles/index.css --dir public/styles --env production",
    "dev": "concurrently \"npm run clean\" \"npm run dev:css\" \"remix dev\"",
    "build": "concurrently \"npm run clean\" \"npm run build:css\" \"remix build\"",
    "deploy": "concurrently \"npm run build\" \"vercel --prod\""
  },
  "dependencies": {
    "@remix-run/node": "^1.7.0",
    "@remix-run/react": "^1.7.0",
    "@remix-run/vercel": "^1.7.0",
    "@vercel/node": "^2.4.4",
    "animejs": "^3.2.1",
    "aos": "^3.0.0-beta.6",
    "clsx": "^1.2.1",
    "framer-motion": "^7.2.1",
    "locomotive-scroll": "^4.1.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.0",
    "@remix-run/eslint-config": "^1.7.0",
    "@remix-run/serve": "^1.7.0",
    "@svgr/core": "^5.5.0",
    "@svgr/plugin-jsx": "^5.5.0",
    "@svgr/plugin-prettier": "^5.5.0",
    "@svgr/plugin-svgo": "^5.5.0",
    "@types/animejs": "^3.1.5",
    "@types/aos": "^3.0.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.8",
    "camelcase": "^5.3.1",
    "concurrently": "^7.3.0",
    "copyfiles": "^2.4.1",
    "cssnano": "^5.1.13",
    "eslint": "^8.20.0",
    "fs": "^0.0.1-security",
    "node-fetch": "^3.2.10",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^15.0.0",
    "rimraf": "^3.0.2",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^9.0.0",
    "tailwindcss": "^3.1.8",
